<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!-- Note: this used to be the complete list of styled controls
     but context-sensitive controls can be 'upgraded' from a simple
     ResourceDictionary to a Styles file with more complex logic, 
     especially referencing parent controls. They are loaded in ThemeRoot.axaml.
-->

  <ResourceDictionary.MergedDictionaries>
    <MergeResourceInclude Source="AutoCompleteBox.axaml" />
    <MergeResourceInclude Source="Button.axaml" />
    <MergeResourceInclude Source="ButtonSpinner.axaml" />
    <MergeResourceInclude Source="Calendar.axaml" />
    <MergeResourceInclude Source="CalendarButton.axaml" />
    <MergeResourceInclude Source="CalendarDatePicker.axaml" />
    <MergeResourceInclude Source="CalendarDayButton.axaml" />
    <MergeResourceInclude Source="CalendarItem.axaml" />
    <MergeResourceInclude Source="CheckBox.axaml" />
    <MergeResourceInclude Source="ClosableTag.axaml" />
    <MergeResourceInclude Source="ComboBox.axaml" />
    <MergeResourceInclude Source="ComboBoxItem.axaml" />
    <MergeResourceInclude Source="DataGrid.axaml" />
    <MergeResourceInclude Source="DropDownButton.axaml" />
    <MergeResourceInclude Source="EmbeddableControlRoot.axaml" />
    <MergeResourceInclude Source="EditableComboBox.axaml" />
    <MergeResourceInclude Source="Expander.axaml" />
    <MergeResourceInclude Source="HyperlinkButton.axaml" />
    <MergeResourceInclude Source="MenuFlyoutPresenter.axaml" />
    <MergeResourceInclude Source="MenuItem.axaml" />
    <MergeResourceInclude Source="MultiComboBox.axaml" />
    <MergeResourceInclude Source="MultiComboBoxItem.axaml" />
    <MergeResourceInclude Source="NumericUpDown.axaml" />
    <MergeResourceInclude Source="Label.axaml" />
    <MergeResourceInclude Source="ListBox.axaml" />
    <MergeResourceInclude Source="ListBoxItem.axaml" />
    <MergeResourceInclude Source="RadioButton.axaml" />
    <MergeResourceInclude Source="ScrollBar.axaml" />
    <MergeResourceInclude Source="ScrollViewer.axaml" />
    <!-- SeparatorStyles.axaml loaded in ThemeRoot.axaml -->
    <MergeResourceInclude Source="SplitButton.axaml" />
    <MergeResourceInclude Source="TabControl.axaml" />
    <MergeResourceInclude Source="TabItem.axaml" />
    <MergeResourceInclude Source="TabPane.axaml" />
    <MergeResourceInclude Source="TagInput.axaml" />
    <MergeResourceInclude Source="TextBox.axaml" />
    <MergeResourceInclude Source="TextBlock.axaml" />
    <MergeResourceInclude Source="TreeView.axaml" />
    <!-- TreeDataGrid.axaml conditionally loaded in DevolutionsDevExpressTheme.cs if licensed Accelerate controls are enabled  -->
    <MergeResourceInclude Source="TreeViewItem.axaml" />
    <MergeResourceInclude Source="ContextMenu.axaml" />
    <MergeResourceInclude Source="Window.axaml" />
  </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>