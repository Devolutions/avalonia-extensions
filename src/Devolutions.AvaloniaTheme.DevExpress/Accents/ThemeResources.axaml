  <SolidColorBrush x:Key="CalendarWeekendForegroundBrush" Color="Red" />
  <x:Double x:Key="CalendarFontSize">11</x:Double>


  <!-- ColorPicker Resources -->
  <VisualBrush x:Key="ColorControlCheckeredBackgroundBrush"
               TileMode="Tile"
               Stretch="Uniform"
               DestinationRect="0,0,8,8">
    <VisualBrush.Visual>
      <Image Width="8" Height="8">
        <Image.Source>
          <DrawingImage>
            <DrawingImage.Drawing>
              <DrawingGroup>
                <GeometryDrawing Geometry="M0,0 L2,0 2,2, 0,2Z" Brush="Transparent" />
                <GeometryDrawing Geometry="M0,1 L2,1 2,2, 1,2 1,0 0,0Z" Brush="#19808080" />
              </DrawingGroup>
            </DrawingImage.Drawing>
          </DrawingImage>
        </Image.Source>
      </Image>
    </VisualBrush.Visual>
  </VisualBrush>

  <SolidColorBrush x:Key="ColorControlLightSelectorBrush" Color="#FFFFFFFF" />
  <SolidColorBrush x:Key="ColorControlDarkSelectorBrush" Color="#E4000000" />

