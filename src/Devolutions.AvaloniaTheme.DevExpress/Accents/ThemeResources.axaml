      <FontWeight x:Key="DefaultFontWeight">Normal</FontWeight>
      <FontWeight x:Key="DemiBoldWeight">DemiBold</FontWeight>

      <!-- HyperlinkButton Resources -->
      <SolidColorBrush x:Key="HyperlinkButtonForeground" Color="{DynamicResource HyperLinkColor}" />
      <SolidColorBrush x:Key="HyperlinkButtonForegroundPointerOver" Color="{DynamicResource HyperLinkColor}" Opacity="0.8" />
      <SolidColorBrush x:Key="HyperlinkButtonForegroundPressed" Color="{DynamicResource HyperLinkColor}" Opacity="0.6" />
