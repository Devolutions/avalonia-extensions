<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <ResourceDictionary.ThemeDictionaries>
    <ResourceDictionary x:Key="Default">
      <Color x:Key="BackgroundColor">#eeffee</Color>
      <Color x:Key="ForegroundColor">#008800</Color>
      <Color x:Key="AccentForegroundColor">#ffff00</Color>
    </ResourceDictionary>

    <ResourceDictionary x:Key="Dark">
      <Color x:Key="BackgroundColor">#004400</Color>
      <Color x:Key="ForegroundColor">#00ff00</Color>
      <Color x:Key="AccentForegroundColor">#ff00ff</Color>
    </ResourceDictionary>
  </ResourceDictionary.ThemeDictionaries>


  <FontFamily x:Key="MacOsThemeFontFamily">Segoe UI, sans-serif</FontFamily>

</ResourceDictionary>