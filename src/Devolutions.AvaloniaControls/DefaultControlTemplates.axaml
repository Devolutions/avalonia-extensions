<Styles
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Styles.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <MergeResourceInclude Source="Colors.axaml" />

        <MergeResourceInclude Source="Controls/InnerIconButton.axaml" />
        <MergeResourceInclude Source="Controls/SearchHighlightTextBlock.axaml" />
        <MergeResourceInclude Source="Controls/EditableComboBox/EditableComboBox.axaml" />
        <MergeResourceInclude Source="Controls/CheckBoxListBox/InnerCheckBoxListBox.axaml" />
        <MergeResourceInclude Source="Controls/CheckBoxListBox/CheckBoxListBoxItem.axaml" />
        <MergeResourceInclude Source="Controls/SmallScrollBar.axaml" />
        <MergeResourceInclude Source="Controls/SmallScrollViewer.axaml" />

        <MergeResourceInclude Source="Controls/ClosableTag.axaml" />
        <MergeResourceInclude Source="Controls/MultiComboBox/MultiComboBox.axaml" />
        <MergeResourceInclude Source="Controls/MultiComboBox/MultiComboBoxItem.axaml" />

        <MergeResourceInclude Source="Controls/ToggleButtonGroup.axaml" />
        <MergeResourceInclude Source="Controls/ToggleButtonGroupItem.axaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>

    <StaticResource x:Key="{x:Type TabPane}" ResourceKey="{x:Type TabControl}" />
  </Styles.Resources>
</Styles>